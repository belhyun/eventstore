<h1>Listing mypeople</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @mypeople.each do |myperson| %>
      <tr>
        <td><%= link_to 'Show', myperson %></td>
        <td><%= link_to 'Edit', edit_myperson_path(myperson) %></td>
        <td><%= link_to 'Destroy', myperson, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Myperson', new_myperson_path %>
